<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="winScreen">
    <property name="height">480</property>
    <property name="width">640</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">False</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.LabelFrame" id="winScreenFrame">
        <property name="font">TkDefaultFont</property>
        <property name="height">480</property>
        <property name="text" translatable="yes">A winner is you!</property>
        <property name="width">640</property>
        <layout>
          <property name="padx">240</property>
          <property name="pady">90</property>
          <property name="propagate">True</property>
        </layout>
        <child>
          <object class="tk.Label" id="winnerMessage">
            <property name="font">TkIconFont</property>
            <property name="highlightthickness">10</property>
            <property name="text" translatable="yes">WINNER!!!</property>
            <property name="textvariable">string:winnerMessage</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="playerName">
            <property name="text" translatable="yes">-place holder-</property>
            <property name="textvariable">string:playerName</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="winMessage">
            <property name="text" translatable="yes"> has become the Destroyer</property>
            <property name="textvariable">string:winMessage</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Canvas" id="windogeCanvas">
            <property name="borderwidth">10</property>
            <property name="height">100</property>
            <property name="relief">raised</property>
            <property name="width">100</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="windoge">
            <property name="text" translatable="yes">Window doge approves!</property>
            <property name="textvariable">string:windoge</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="Attacks dealt:">
            <property name="text" translatable="yes">Attacks dealt:</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="Floors reached">
            <property name="text" translatable="yes">Floors reached:</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="Destroyers destroyed:">
            <property name="text" translatable="yes">Destroyers destroyed:</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">7</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="atkHolder">
            <property name="text" translatable="yes">atkHolder</property>
            <property name="textvariable">string:atkHolder</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="floHolder">
            <property name="text" translatable="yes">floHolder</property>
            <property name="textvariable">string:floHolder</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="ddHolder">
            <property name="compound">bottom</property>
            <property name="text" translatable="yes">ddHolder</property>
            <property name="textvariable">string:ddHolder</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">7</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="mainMenuButton">
            <property name="command">mainMenu</property>
            <property name="padx">9</property>
            <property name="text" translatable="yes">MainMenu</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">8</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="NewGameButton">
            <property name="command">newGame</property>
            <property name="padx">10</property>
            <property name="text" translatable="yes">NewGame</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">9</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Frame" id="creditsScreen">
