  <object class="tk.Menu" id="menuBar">
    <property name="tearoff">false</property>
    <child>
      <object class="tk.Menuitem.Submenu" id="file">
        <property name="label" translatable="yes">File</property>
        <property name="tearoff">false</property>
        <child>
          <object class="tk.Menuitem.Command" id="fileMainMenu">
            <property name="command">menuBarMainMenu</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">MainMenu</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="fileNewGame">
            <property name="command">menuBarNewGame</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">NewGame</property>
