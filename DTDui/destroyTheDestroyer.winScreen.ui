<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="winScreen">
    <property name="height">640</property>
    <property name="width">480</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.LabelFrame" id="winScreenFrame">
        <property name="height">640</property>
        <property name="text" translatable="yes">A winner is you!</property>
        <property name="width">480</property>
        <layout>
          <property name="column">3</property>
          <property name="padx">220</property>
          <property name="pady">100</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
        <child>
          <object class="tk.Label" id="winnerMessage">
            <property name="font">TkIconFont</property>
            <property name="highlightthickness">10</property>
            <property name="text" translatable="yes">WINNER!!!</property>
            <property name="textvariable">string:winnerMessage</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="playerName">
            <property name="text" translatable="yes">-place holder-</property>
            <property name="textvariable">string:playerName</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="winMessage">
            <property name="text" translatable="yes"> has become the Destroyer</property>
            <property name="textvariable">string:winMessage</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="formating">
            <property name="height">100</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="windoge">
            <property name="text" translatable="yes">Window doge approves!</property>
            <property name="textvariable">string:windoge</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="mainMenuButton">
            <property name="command">mainMenu</property>
            <property name="text" translatable="yes">MainMenu</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Frame" id="creditsScreen">
    <property name="height">640</property>
    <property name="width">480</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.LabelFrame" id="creditsScreenFrame">
        <property name="height">640</property>
        <property name="text" translatable="yes">Credits</property>
        <property name="width">480</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">190</property>
          <property name="pady">100</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="tk.Label" id="creditLabel">
            <property name="text" translatable="yes">DestroyTheDestroyer was concieved and built by: </property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="Billy">
            <property name="text" translatable="yes">Billy Gale: Billy bool boy</property>
            <property name="textvariable">string:Billy</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="Kyle">
            <property name="text" translatable="yes">Kyle Gunton: Kyle the Destroyer</property>
            <property name="textvariable">string:Kyle</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="Ryan">
            <property name="text" translatable="yes">Ryan Breau: Ryan not Reynolds</property>
            <property name="textvariable">string:Ryan</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="Zach">
            <property name="text" translatable="yes">Zachary Blue: Zach attack</property>
            <property name="textvariable">string:Zack</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="Elie">
            <property name="text" translatable="yes">Elie Godbout: Elustrious</property>
            <property name="textvariable">string:Elie</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="mainMenuButton_1">
            <property name="command">mainMenu</property>
            <property name="text" translatable="yes">MainMenu</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Menu" id="menuBar">
    <property name="tearoff">false</property>
    <child>
      <object class="tk.Menuitem.Submenu" id="file">
        <property name="label" translatable="yes">File</property>
        <property name="tearoff">false</property>
        <child>
          <object class="tk.Menuitem.Command" id="fileMainMenu">
            <property name="command">menuBarMainMenu</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">MainMenu</property>
          </object>
        </child>
        <child>
          <object class="tk.Menuitem.Command" id="fileQuit">
            <property name="command">menuBarQuit</property>
            <property name="command_id_arg">false</property>
            <property name="label" translatable="yes">Quit</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
