<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="LevelUp">
    <property name="height">480</property>
    <property name="padx">138</property>
    <property name="pady">30</property>
    <property name="width">640</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">False</property>
      <property name="row">0</property>
      <columns>
        <column id="0">
          <property name="pad">0</property>
        </column>
        <column id="1">
          <property name="pad">0</property>
        </column>
      </columns>
      <rows>
        <row id="0">
          <property name="minsize">0</property>
          <property name="pad">36</property>
        </row>
      </rows>
    </layout>
    <child>
      <object class="tk.Frame" id="Frame_1">
        <property name="height">480</property>
        <property name="width">640</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">10</property>
          <property name="propagate">False</property>
          <property name="row">0</property>
          <property name="rowspan">10</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btnConfirm">
        <property name="command">confirm</property>
        <property name="state">disabled</property>
        <property name="text" translatable="yes">Confirm</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Frame_2">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
        <child>
          <object class="tk.Label" id="lblItem">
            <property name="pady">0</property>
            <property name="text" translatable="yes">Item Get:                  x (1-3)          </property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">1</property>
              <property name="ipady">11</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="lblFloorProg">
            <property name="text" translatable="yes">Current Floor --&gt; Next Floor</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="ipady">11</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Canvas" id="canPlayer">
            <property name="borderwidth">5</property>
            <property name="height">96</property>
            <property name="relief">raised</property>
            <property name="width">96</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">3</property>
              <property name="propagate">False</property>
              <property name="row">2</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="lblPots">
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Frame_4">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
        <child>
          <object class="tk.Label" id="lblLvl">
            <property name="text" translatable="yes">lvl.Current --&gt; lvl.Next</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="lblLevelUp">
            <property name="text" translatable="yes">Level Up!!!!</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="frmStats">
            <property name="borderwidth">2</property>
            <property name="height">180</property>
            <property name="relief">sunken</property>
            <property name="width">80</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">1</property>
              <property name="propagate">False</property>
              <property name="row">2</property>
              <columns>
                <column id="0">
                  <property name="minsize">39</property>
                  <property name="pad">0</property>
                  <property name="weight">0</property>
                </column>
              </columns>
              <rows>
                <row id="0">
                  <property name="minsize">0</property>
                  <property name="weight">0</property>
                </row>
                <row id="1">
                  <property name="minsize">0</property>
                </row>
                <row id="2">
                  <property name="minsize">0</property>
                </row>
                <row id="5">
                  <property name="pad">0</property>
                </row>
              </rows>
            </layout>
            <child>
              <object class="tk.Label" id="lblCurrentHP">
                <property name="text" translatable="yes"> HP: 00 </property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">3</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="lblCurrentATK">
                <property name="text" translatable="yes">ATK: 00</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">3</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="lblCurrentDEF">
                <property name="text" translatable="yes">DEF: 00</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">3</property>
                  <property name="ipadx">14</property>
                  <property name="propagate">True</property>
                  <property name="row">6</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="btnHpUp">
                <property name="command">hpUp</property>
                <property name="text" translatable="yes">+</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="btnHpDown">
                <property name="command">hpDown</property>
                <property name="state">disabled</property>
                <property name="text" translatable="yes">-</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="btnAtkUp">
                <property name="command">atkUp</property>
                <property name="text" translatable="yes">+</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="btnAtkDown">
                <property name="command">atkDown</property>
                <property name="state">disabled</property>
                <property name="text" translatable="yes">-</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="btnDefUp">
                <property name="command">defUp</property>
                <property name="text" translatable="yes">+</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">8</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="btnDefDown">
                <property name="command">defDown</property>
                <property name="state">disabled</property>
                <property name="text" translatable="yes">-</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">8</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="lblGained">
                <property name="text" translatable="yes">Stat: 00</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="padx">15</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="rowspan">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
