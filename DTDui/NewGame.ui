<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="NewGame">
    <property name="height">480</property>
    <property name="width">640</property>
    <layout>
      <property name="column">1</property>
      <property name="columnspan">8</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="rowspan">8</property>
      <property name="sticky">e</property>
    </layout>
    <child>
      <object class="tk.Label" id="labelPlayerName">
        <property name="text" translatable="yes">Name :</property>
        <property name="textvariable">string:labelPlayerName</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="padx">20</property>
          <property name="pady">40</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="entryPlayerName">
        <property name="textvariable">string:enterPlayerName</property>
        <layout>
          <property name="column">1</property>
          <property name="columnspan">1</property>
          <property name="padx">40</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="rowspan">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="startDificulty">
        <property name="command">startDifficultySelect</property>
        <property name="text" translatable="yes">Start</property>
        <property name="underline">0</property>
        <layout>
          <property name="column">1</property>
          <property name="columnspan">1</property>
          <property name="padx">20</property>
          <property name="pady">100</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
          <property name="rowspan">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Listbox" id="playerClassList">
        <property name="listvariable">string:playerClassList</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.LabelFrame" id="ClassDetails">
        <property name="height">200</property>
        <property name="text" translatable="yes">Class Starting Stats</property>
        <property name="width">200</property>
        <layout>
          <property name="column">2</property>
          <property name="columnspan">1</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="rowspan">1</property>
          <columns>
            <column id="0">
              <property name="minsize">200</property>
            </column>
          </columns>
          <rows>
            <row id="0">
              <property name="minsize">129</property>
            </row>
          </rows>
        </layout>
        <child>
          <object class="tk.Label" id="classDetails">
            <property name="justify">left</property>
            <layout>
              <property name="column">0</property>
              <property name="ipadx">10</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Canvas" id="PlayerImage">
        <property name="height">96</property>
        <property name="width">96</property>
        <layout>
          <property name="column">2</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
