<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="frmSignUp">
    <property name="height">960</property>
    <property name="width">1280</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">False</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="tk.Canvas" id="canBanner">
        <property name="height">200</property>
        <property name="selectborderwidth">0</property>
        <property name="width">1280</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">False</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="frmInput">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="pady">20</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
        <child>
          <object class="tk.Label" id="lblCreateAccount">
            <property name="height">0</property>
            <property name="height">0</property>
            <property name="text" translatable="yes">Create an Account</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="lblPlayerID">
            <property name="text" translatable="yes">Enter your Puzzle and Dragons player ID:</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="entPlayerID">
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="lblUsername">
            <property name="text" translatable="yes">Enter your username:</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="entUsername">
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="lblPassword">
            <property name="text" translatable="yes">Enter your password:</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="entPassword">
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="lblPasswordConfirm">
            <property name="text" translatable="yes">Re-enter your password:</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">7</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="entPasswordConfirm">
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">8</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="lblEmail">
            <property name="text" translatable="yes">Enter your email:</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">9</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="entEmail">
            <layout>
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="propagate">True</property>
              <property name="row">10</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="btnCreate">
            <property name="command">CreateAccount</property>
            <property name="text" translatable="yes">Create Account</property>
            <property name="width">13</property>
            <layout>
              <property name="column">0</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">11</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="btnCancel">
            <property name="command">Cancel</property>
            <property name="text" translatable="yes">Cancel</property>
            <property name="width">13</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">False</property>
              <property name="row">11</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
