<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="Login Screen">
    <property name="height">960</property>
    <property name="width">1280</property>
    <layout>
      <property name="columnspan">10</property>
      <property name="propagate">False</property>
      <property name="rowspan">10</property>
    </layout>
    <child>
      <object class="tk.Button" id="Login">
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Login</property>
        <property name="width">16</property>
        <bind add="True" handler="onLoginClick" sequence="&lt;1&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="padx">640</property>
          <property name="pady">20</property>
          <property name="propagate">False</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="Create Account">
        <property name="text" translatable="yes">Create Account</property>
        <property name="width">16</property>
        <bind add="" handler="onCreateAccountClick" sequence="&lt;1&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">False</property>
          <property name="row">4</property>
          <property name="sticky">s</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="entEmail">
        <property name="foreground">#c6caca</property>
        <property name="text" translatable="yes">Enter Email</property>
        <property name="textvariable">string:varEmail</property>
        <bind add="True" handler="onEmailFocusIn" sequence="&lt;FocusIn&gt;" />
        <bind add="True" handler="onEmailFocusOut" sequence="&lt;FocusOut&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="padx">0</property>
          <property name="pady">20</property>
          <property name="propagate">False</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="entPassword">
        <property name="foreground">#c6caca</property>
        <property name="text" translatable="yes">Enter Password</property>
        <property name="textvariable">string:varPassword</property>
        <bind add="True" handler="onPasswordFocusIn" sequence="&lt;FocusIn&gt;" />
        <bind add="True" handler="onPasswordFocusOut" sequence="&lt;FocusOut&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">False</property>
          <property name="row">2</property>
          <property name="sticky">s</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Canvas" id="TitleCanvas">
        <property name="height">200</property>
        <property name="takefocus">true</property>
        <property name="width">1280</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">False</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
