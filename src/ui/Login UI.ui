<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="Login Screen">
    <property name="height">960</property>
    <property name="width">1280</property>
    <layout>
      <property name="columnspan">10</property>
      <property name="propagate">False</property>
      <property name="rowspan">10</property>
    </layout>
    <child>
      <object class="tk.Canvas" id="Canvas_3">
        <property name="height">960</property>
        <property name="takefocus">false</property>
        <property name="width">1280</property>
        <layout>
          <property name="columnspan">10</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="rowspan">10</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="Login">
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Login</property>
        <bind add="True" handler="onLoginClick" sequence="&lt;1&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="padx">640</property>
          <property name="propagate">False</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="Create Account">
        <property name="command">onCreateAccountClick</property>
        <property name="text" translatable="yes">Create Account</property>
        <bind add="True" handler="onCreateAccountClick" sequence="&lt;1&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">False</property>
          <property name="row">2</property>
          <property name="sticky">s</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="Username">
        <property name="text" translatable="yes">Username</property>
        <property name="textvariable">string:Username</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">False</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="Password">
        <property name="text" translatable="yes">Password</property>
        <property name="textvariable">string:Password</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">False</property>
          <property name="row">1</property>
          <property name="sticky">s</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Canvas" id="TitleCanvas">
        <property name="height">200</property>
        <property name="takefocus">true</property>
        <property name="width">1280</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">False</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
