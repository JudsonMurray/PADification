<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="pygubu.builder.widgets.tkscrolledframe" id="EditTeamFrame">
    <property name="height">960</property>
    <property name="padx">0</property>
    <property name="scrolltype">vertical</property>
    <property name="width">1280</property>
    <layout>
      <property name="column">0</property>
      <property name="columnspan">1</property>
      <property name="ipadx">640</property>
      <property name="ipady">480</property>
      <property name="propagate">False</property>
      <property name="rowspan">1</property>
    </layout>
    <child>
      <object class="tk.Canvas" id="titleImage">
        <property name="height">200</property>
        <property name="highlightbackground">#cc7d75</property>
        <property name="selectbackground">#a5aa97</property>
        <property name="selectforeground">#b6d56c</property>
        <property name="takefocus">true</property>
        <property name="width">1261</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lblPadInfoBar">
        <property name="height">5</property>
        <property name="height">5</property>
        <property name="relief">sunken</property>
        <property name="text" translatable="yes">PADification Application Information</property>
        <property name="width">170</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="padx">40</property>
          <property name="pady">20</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="pygubu.builder.widgets.tkscrolledframe" id="monsterCollectionFrame">
        <property name="background">#6b7289</property>
        <property name="relief">raised</property>
        <property name="scrolltype">vertical</property>
        <layout>
          <property name="column">0</property>
          <property name="ipadx">300</property>
          <property name="ipady">200</property>
          <property name="propagate">False</property>
          <property name="row">1</property>
          <property name="sticky">n</property>
        </layout>
        <child>
          <object class="tk.Canvas" id="canMonsterCollection">
            <property name="height">520</property>
            <property name="width">640</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="TeamEditFrame">
        <property name="height">600</property>
        <property name="relief">groove</property>
        <property name="width">400</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">s</property>
        </layout>
        <child>
          <object class="tk.Canvas" id="canEditTeam">
            <property name="height">520</property>
            <property name="insertborderwidth">0</property>
            <property name="width">680</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">10</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="rowspan">10</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Canvas" id="canLeadMon">
            <property name="background">#decbf3</property>
            <property name="borderwidth">5</property>
            <property name="height">70</property>
            <property name="relief">raised</property>
            <property name="width">70</property>
            <bind add="True" handler="leadClick" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">0</property>
              <property name="padx">20</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Canvas" id="canSubMon1">
            <property name="background">#d3baef</property>
            <property name="borderwidth">5</property>
            <property name="height">70</property>
            <property name="relief">raised</property>
            <property name="width">70</property>
            <bind add="True" handler="sub1Click" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">1</property>
              <property name="padx">20</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Canvas" id="canSubMon2">
            <property name="background">#a470de</property>
            <property name="borderwidth">5</property>
            <property name="height">70</property>
            <property name="relief">raised</property>
            <property name="width">70</property>
            <bind add="True" handler="sub2Click" sequence="&lt;1&gt;" />
