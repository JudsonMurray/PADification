            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lblLightATK">
            <property name="text" translatable="yes">Light ATK:</property>
            <layout>
              <property name="column">01</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="darkImg">
            <property name="text" translatable="yes">Label_7</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">6</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lblDarkATK">
            <property name="text" translatable="yes">Dark ATK:</property>
            <layout>
              <property name="column">01</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="btnRemoveMonster">
            <property name="text" translatable="yes">Remove Monster</property>
            <bind add="True" handler="removeMonster" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">7</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
