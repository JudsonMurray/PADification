            <layout>
              <property name="column">1</property>
              <property name="propagate">False</property>
              <property name="row">3</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="woodImg">
            <property name="text" translatable="yes">Label_5</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">False</property>
              <property name="row">4</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lblWoodATK">
            <property name="text" translatable="yes">Wood ATK:</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">False</property>
              <property name="row">4</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
