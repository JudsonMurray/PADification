              <property name="column">2</property>
              <property name="padx">20</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Canvas" id="canSubMon3">
            <property name="background">#6d2bb7</property>
            <property name="borderwidth">5</property>
            <property name="height">70</property>
            <property name="relief">raised</property>
            <property name="width">70</property>
            <bind add="True" handler="sub3Click" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">3</property>
              <property name="padx">20</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Canvas" id="canSubMon4">
            <property name="background">#3e1867</property>
            <property name="borderwidth">5</property>
            <property name="height">70</property>
            <property name="relief">raised</property>
            <property name="width">70</property>
            <bind add="True" handler="sub4Click" sequence="&lt;1&gt;" />
            <layout>
              <property name="column">4</property>
              <property name="padx">20</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Radiobutton" id="teamMonsterSelected">
            <property name="takefocus">false</property>
            <property name="textvariable">string:monSelect</property>
            <property name="value">0</property>
            <property name="variable">string:monsterSelected</property>
            <layout>
              <property name="column">6</property>
              <property name="padx">100</property>
              <property name="propagate">False</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lblTeamHP">
            <property name="text" translatable="yes">HP:</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lblTeamRcv">
            <property name="text" translatable="yes">RCV:</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
